    "Marcus Neble Jensen",
    "Mathias Br√¶ndgaard",
    "Omar Suleiman",
    "Vanessa Scherma"
  ],
  "license": "SEE LICENSE IN <LICENSE.md>",
  "private": false,
  "type": "module",
  "scripts": {
    "analyze": "yarn build && npx source-map-explorer 'build/static/js/*.js'",
    "build": "cross-env DISABLE_ESLINT_PLUGIN=true npx react-scripts build",
    "build:fast": "cross-env DISABLE_ESLINT_PLUGIN=true GENERATE_SOURCEMAP=false npx react-scripts build",
    "build:prod": "cross-env DISABLE_ESLINT_PLUGIN=true GENERATE_SOURCEMAP=false npx react-scripts build",
    "clean": "npx rimraf build/ dist/ coverage/ playwright-report/ test-results/ test.svg src.svg",
    "clean:all": "yarn clean && npx rimraf node_modules/",
    "config:dev": "npx shx cp config/dev.js public/env.js && npx shx cp config/dev.js build/env.js",
    "config:local": "npx shx cp config/local.js public/env.js && npx shx cp config/local.js build/env.js",
    "config:prod": "npx shx cp config/prod.js public/env.js && npx shx cp config/prod.js build/env.js",
    "config:test": "npx shx cp config/test.js public/env.js && npx shx cp config/test.js build/env.js",
    "develop": "cross-env PORT=4000 npx react-scripts start",
    "develop:fast": "cross-env PORT=4000 FAST_REFRESH=true BROWSER=none TSC_COMPILE_ON_ERROR=true ESLINT_NO_DEV_ERRORS=true npx react-scripts start",
    "format": "prettier --ignore-path ../.gitignore --write \"**/*.{ts,tsx,css,scss}\"",
    "graph": "npx madge --image src.svg src && npx madge --image test.svg test",
    "start": "serve -s build -l 4000",
    "stop": "npx kill-port 4000",
    "syntax": "npx eslint . --fix",
    "test:all": "yarn test:unit && yarn test:int && yarn test:preview:unit && yarn test:preview:int && yarn test:e2e",
    "test:e2e:ext": "cross-env ext=true yarn test:e2e",
    "test:e2e": "yarn config:test && yarn build && playwright test -c ./playwright.config.ts",
    "test:coverage:int-unit": "npx istanbul-merge --out coverage/all/coverage.json coverage/unit/coverage-final.json coverage/int/coverage-final.json coverage/preview/unit/coverage-final.json coverage/preview/int/coverage-final.json && npx nyc report --reporter=lcov --reporter=text --reporter=json -t coverage/all --report-dir coverage/all",
    "test:int": "jest -c ./jest.config.json --coverage --coverageDirectory=coverage/int ../test/integration --setupFilesAfterEnv ./test/integration/jest.setup.ts",
    "test:unit": "jest -c ./jest.config.json --coverageDirectory=coverage/unit ../test/unit --setupFilesAfterEnv ./test/unit/jest.setup.ts",
    "test:preview:int": "jest -c ./jest.config.json --runInBand --coverageDirectory=coverage/preview/int ../test/preview/integration --setupFilesAfterEnv ./test/preview/integration/jest.setup.ts",
    "test:preview:unit": "jest -c ./jest.config.json --runInBand --coverageDirectory=coverage/preview/unit ../test/preview/unit --setupFilesAfterEnv ./test/preview/unit/jest.setup.ts"
  },
  "prettier": {
    "singleQuote": true
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine@^7.3.5",
    "react-is": "^19.2.0",
    "node-forge": "^1.3.2"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.8",
    "@gitbeaker/rest": "^43.8.0",
    "@monaco-editor/react": "4.7.0",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/system": "^7.3.5",
    "@mui/x-tree-view": "^8.18.0",
    "@reduxjs/toolkit": "^2.10.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/remarkable": "^2.0.8",
    "@types/styled-components": "^5.1.36",
    "@types/uuid": "10.0.0",
    "cross-env": "7.0.3",
    "dotenv": "^17.2.3",
    "http-status-codes": "2.3.0",
    "katex": "^0.16.25",
    "monaco-editor": "^0.52.2",
    "oidc-client-ts": "^3.4.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-iframe": "^1.8.5",
    "react-oidc-context": "^3.3.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.6",
    "react-scripts": "^5.0.1",
    "react-syntax-highlighter": "^16.1.0",
    "react-tabs": "^6.1.0",
    "redux": "^5.0.1",
    "remarkable": "^2.0.1",
    "remarkable-katex": "^1.2.1",
    "reselect": "^5.1.1",
    "serve": "^14.2.5",
    "styled-components": "^6.1.19",
